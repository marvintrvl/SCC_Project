{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      cartItems: [],\n      cartTotal: 0\n    };\n  },\n  mounted() {\n    this.fetchCart();\n  },\n  methods: {\n    fetchCart() {\n      const authToken = this.$store.state.authToken;\n      if (!authToken) {\n        console.error('No authentication token found.');\n        return;\n      }\n      axios.get('http://127.0.0.1:5002/get-cart', {\n        headers: {\n          Authorization: `Bearer ${authToken}`\n        }\n      }).then(response => {\n        console.log('Cart data from the server:', response.data);\n        this.cartItems = response.data.cart_items;\n        this.cartTotal = response.data.total_price;\n      }).catch(error => {\n        console.error('Error fetching cart:', error);\n        console.log('Request details:', error.request);\n        console.log('Response details:', error.response);\n      });\n    },\n    emptyCart() {\n      // Implement logic to empty the cart\n      axios.delete('http://127.0.0.1:5002/clear-cart', {\n        headers: {\n          Authorization: `Bearer ${this.$store.state.authToken}`\n        }\n      }).then(() => {\n        this.fetchCart(); // Refresh the cart after clearing\n        alert('Cart emptied successfully!');\n      }).catch(error => {\n        console.error('Error emptying cart:', error);\n      });\n    },\n    updateQuantity(itemId) {\n      // Implement logic to update the quantity\n      const form = document.getElementById(`update-form-${itemId}`);\n      const quantity = form.elements[\"quantity\"];\n\n      // Send a request to update the quantity\n      axios.put(`http://127.0.0.1:5002/update-cart-item/${itemId}`, {\n        quantity: parseInt(quantity.value)\n      }, {\n        headers: {\n          Authorization: `Bearer ${this.$store.state.authToken}`\n        }\n      }).then(() => {\n        this.fetchCart(); // Refresh the cart after updating\n      }).catch(error => {\n        console.error('Error updating quantity:', error);\n      });\n    },\n    deleteItem(itemId) {\n      // Implement logic to delete an item from the cart\n      axios.delete(`http://127.0.0.1:5002/delete-cart-item/${itemId}`, {\n        headers: {\n          Authorization: `Bearer ${this.$store.state.authToken}`\n        }\n      }).then(() => {\n        this.fetchCart(); // Refresh the cart after deleting\n        alert('Item deleted from the cart successfully!');\n      }).catch(error => {\n        console.error('Error deleting item:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","cartItems","cartTotal","mounted","fetchCart","methods","authToken","$store","state","console","error","get","headers","Authorization","then","response","log","cart_items","total_price","catch","request","emptyCart","delete","alert","updateQuantity","itemId","form","document","getElementById","quantity","elements","put","parseInt","value","deleteItem"],"sources":["C:\\Users\\menzm\\Desktop\\SCC_Project\\frontend\\src\\views\\ShoppingCartView.vue"],"sourcesContent":["<template>\r\n  <div class=\"max-w-3xl mx-auto mt-8\">\r\n    <h1 class=\"text-3xl font-semibold mb-6\">Your Shopping Cart</h1>\r\n\r\n    <form @submit.prevent=\"emptyCart\">\r\n      <button type=\"submit\" class=\"bg-red-500 hover:bg-red-700 text-white font-bold py-2 px-4 rounded\">\r\n        Empty Cart\r\n      </button>\r\n    </form>\r\n\r\n    <table class=\"w-full border-collapse border border-gray-300 mt-4\">\r\n      <thead>\r\n        <tr>\r\n          <th class=\"p-2 border\">Item</th>\r\n          <th class=\"p-2 border\">Size</th>\r\n          <th class=\"p-2 border\">Quantity</th>\r\n          <th class=\"p-2 border\">Subtotal</th>\r\n          <th class=\"p-2 border\">Actions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"item in cartItems\" :key=\"item.id\" class=\"border-b\">\r\n          <td class=\"p-2 border\">\r\n            <div class=\"flex items-center\">\r\n              <div>\r\n                <p class=\"font-bold\">{{ item.photo.name }}</p>\r\n                <p>{{ item.photo.title }}</p>\r\n              </div>\r\n              <img :src=\"`http://127.0.0.1:5001/${item.photo.image}`\" :alt=\"item.photo.name\" class=\"w-40 h-40 object-contain rounded-md ml-4\">\r\n            </div>\r\n          </td>\r\n          <td class=\"p-2 border\">{{ item.size }}</td>\r\n          <td class=\"p-2 border\">\r\n            <form :id=\"'update-form-' + item.id\" @submit.prevent=\"updateQuantity(item.id)\">\r\n              <input type=\"number\" v-model=\"item.quantity\" min=\"1\">\r\n            </form>\r\n          </td>\r\n          <td class=\"p-2 border\">${{ item.subtotal }}</td>\r\n          <td class=\"p-2 border\">\r\n            <button @click=\"deleteItem(item.id)\" class=\"text-red-500 hover:text-red-700\">Delete</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <p class=\"mt-4\">Total: ${{ cartTotal }}</p>\r\n    <br>\r\n    <router-link to=\"/checkout\" class=\"mt-4 bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded\">\r\n      Proceed to Checkout\r\n    </router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      cartItems: [],\r\n      cartTotal: 0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchCart();\r\n  },\r\n  methods: {\r\n    fetchCart() {\r\n      const authToken = this.$store.state.authToken;\r\n      if (!authToken) {\r\n        console.error('No authentication token found.');\r\n        return;\r\n      }\r\n\r\n      axios.get('http://127.0.0.1:5002/get-cart', {\r\n        headers: {\r\n          Authorization: `Bearer ${authToken}`,\r\n        },\r\n      })\r\n        .then(response => {\r\n          console.log('Cart data from the server:', response.data);\r\n          this.cartItems = response.data.cart_items;\r\n          this.cartTotal = response.data.total_price;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching cart:', error);\r\n          console.log('Request details:', error.request);\r\n          console.log('Response details:', error.response);\r\n        });\r\n    },\r\n    emptyCart() {\r\n      // Implement logic to empty the cart\r\n      axios.delete('http://127.0.0.1:5002/clear-cart', {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$store.state.authToken}`,\r\n        },\r\n      })\r\n        .then(() => {\r\n          this.fetchCart(); // Refresh the cart after clearing\r\n          alert('Cart emptied successfully!');\r\n        })\r\n        .catch(error => {\r\n          console.error('Error emptying cart:', error);\r\n        });\r\n    },\r\n    updateQuantity(itemId) {\r\n      // Implement logic to update the quantity\r\n      const form = document.getElementById(`update-form-${itemId}`);\r\n      const quantity = form.elements[\"quantity\"];\r\n\r\n      // Send a request to update the quantity\r\n      axios.put(`http://127.0.0.1:5002/update-cart-item/${itemId}`, { quantity: parseInt(quantity.value) }, {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$store.state.authToken}`,\r\n        },\r\n      })\r\n        .then(() => {\r\n          this.fetchCart(); // Refresh the cart after updating\r\n        })\r\n        .catch(error => {\r\n          console.error('Error updating quantity:', error);\r\n        });\r\n    },\r\n    deleteItem(itemId) {\r\n      // Implement logic to delete an item from the cart\r\n      axios.delete(`http://127.0.0.1:5002/delete-cart-item/${itemId}`, {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$store.state.authToken}`,\r\n        },\r\n      })\r\n        .then(() => {\r\n          this.fetchCart(); // Refresh the cart after deleting\r\n          alert('Item deleted from the cart successfully!');\r\n        })\r\n        .catch(error => {\r\n          console.error('Error deleting item:', error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n"],"mappings":"AAsDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPD,SAASA,CAAA,EAAG;MACV,MAAME,SAAQ,GAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,SAAS;MAC7C,IAAI,CAACA,SAAS,EAAE;QACdG,OAAO,CAACC,KAAK,CAAC,gCAAgC,CAAC;QAC/C;MACF;MAEAX,KAAK,CAACY,GAAG,CAAC,gCAAgC,EAAE;QAC1CC,OAAO,EAAE;UACPC,aAAa,EAAG,UAASP,SAAU;QACrC;MACF,CAAC,EACEQ,IAAI,CAACC,QAAO,IAAK;QAChBN,OAAO,CAACO,GAAG,CAAC,4BAA4B,EAAED,QAAQ,CAACf,IAAI,CAAC;QACxD,IAAI,CAACC,SAAQ,GAAIc,QAAQ,CAACf,IAAI,CAACiB,UAAU;QACzC,IAAI,CAACf,SAAQ,GAAIa,QAAQ,CAACf,IAAI,CAACkB,WAAW;MAC5C,CAAC,EACAC,KAAK,CAACT,KAAI,IAAK;QACdD,OAAO,CAACC,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;QAC5CD,OAAO,CAACO,GAAG,CAAC,kBAAkB,EAAEN,KAAK,CAACU,OAAO,CAAC;QAC9CX,OAAO,CAACO,GAAG,CAAC,mBAAmB,EAAEN,KAAK,CAACK,QAAQ,CAAC;MAClD,CAAC,CAAC;IACN,CAAC;IACDM,SAASA,CAAA,EAAG;MACV;MACAtB,KAAK,CAACuB,MAAM,CAAC,kCAAkC,EAAE;QAC/CV,OAAO,EAAE;UACPC,aAAa,EAAG,UAAS,IAAI,CAACN,MAAM,CAACC,KAAK,CAACF,SAAU;QACvD;MACF,CAAC,EACEQ,IAAI,CAAC,MAAM;QACV,IAAI,CAACV,SAAS,CAAC,CAAC,EAAE;QAClBmB,KAAK,CAAC,4BAA4B,CAAC;MACrC,CAAC,EACAJ,KAAK,CAACT,KAAI,IAAK;QACdD,OAAO,CAACC,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDc,cAAcA,CAACC,MAAM,EAAE;MACrB;MACA,MAAMC,IAAG,GAAIC,QAAQ,CAACC,cAAc,CAAE,eAAcH,MAAO,EAAC,CAAC;MAC7D,MAAMI,QAAO,GAAIH,IAAI,CAACI,QAAQ,CAAC,UAAU,CAAC;;MAE1C;MACA/B,KAAK,CAACgC,GAAG,CAAE,0CAAyCN,MAAO,EAAC,EAAE;QAAEI,QAAQ,EAAEG,QAAQ,CAACH,QAAQ,CAACI,KAAK;MAAE,CAAC,EAAE;QACpGrB,OAAO,EAAE;UACPC,aAAa,EAAG,UAAS,IAAI,CAACN,MAAM,CAACC,KAAK,CAACF,SAAU;QACvD;MACF,CAAC,EACEQ,IAAI,CAAC,MAAM;QACV,IAAI,CAACV,SAAS,CAAC,CAAC,EAAE;MACpB,CAAC,EACAe,KAAK,CAACT,KAAI,IAAK;QACdD,OAAO,CAACC,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD,CAAC,CAAC;IACN,CAAC;IACDwB,UAAUA,CAACT,MAAM,EAAE;MACjB;MACA1B,KAAK,CAACuB,MAAM,CAAE,0CAAyCG,MAAO,EAAC,